{{#modal-dialog close="back"
                targetAttachment="center"
                translucentOverlay=true}}
  <div class="modal-body">
    <table>
      {{#each magnets as |magnet|}}
        <tr>
          <td>{{magnet.title}}</td>
          <td>{{magnet.seeders}}</td>
          <td>
            <a {{action "play" magnet}} href="#">
              <i class="fa fa-play"></i>
            </a>
          </td>
        </tr>
      {{/each}}
    </table>
  </div>
{{/modal-dialog}}